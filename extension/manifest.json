{
  "manifest_version": 3,
  "name": "Image-Translator",
  "description": "translate images to english or desired language",
  "version":"1.0",
  "action":{
      "default_popup": "popup/popup.html"
  },
  "permissions":[
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions":[
    "http://localhost:3000/*"
  ],
  "icons":{
      "16": "images/16.png",
      "32": "images/32.png",
      "128": "images/128.png"
  }
}